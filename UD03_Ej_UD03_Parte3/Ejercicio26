base = float(input("Introduce la base: "))
exponente = int(input("Introduce el exponente: "))
if exponente > 0:
    resultado = 1
    for _ in range(exponente):
        resultado *= base
    print(resultado)
elif exponente == 0:
    print(1)
else:
    resultado = 1
    for _ in range(-exponente):
        resultado *= base
    print(1 / resultado)
