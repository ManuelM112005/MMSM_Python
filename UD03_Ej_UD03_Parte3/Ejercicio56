import math
def es_primo(num):
    if num < 2:
        return False
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0:
            return False
    return True

N = int(input("Introduce cuántos números primos quieres mostrar: "))
encontrados = 0
num = 2
while encontrados < N:
    if es_primo(num):
        print(num)
        encontrados += 1
    num += 1