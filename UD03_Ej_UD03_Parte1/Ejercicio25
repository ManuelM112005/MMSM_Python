nombre = input("Escribe tu nombre: ")
facultad = input("Escribe tu facultad: ")
if facultad == "Ing. de Sistemas":
    importe = 350
    mensualidad = 650
elif facultad == "Derecho":
    importe = 300
    mensualidad = 550
elif facultad == "Ing. Naviera":
    importe = 300
    mensualidad = 500
elif facultad == "Ing. Pesquera":
    importe = 310
    mensualidad = 460
elif facultad == "Contabilidad":
    importe = 280
    mensualidad = 490
elif facultad == "Administraci√≥n":
    importe = 360
    mensualidad = 520
else:
    importe = 0
    mensualidad = 0
subtotal = importe + mensualidad
igv = subtotal * 0.18
total = subtotal + igv
print(importe)
print(mensualidad)
print(igv)
print(total)